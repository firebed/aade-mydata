{
  "name": "firebed/aade-mydata",
  "type": "package",
  "description": "Implementation of requests for AADE myDATA.",
  "keywords": [
    "myDATA",
    "AADE",
    "ΑΑΔΕ"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Okan Giritli",
      "email": "okan.giritli@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "firebed/vat-registry": "^1.0",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Firebed\\AadeMyData\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
